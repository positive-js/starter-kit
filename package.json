        "stylelint": "^13.13.1",
        "stylelint-scss": "^3.20.1",
        "ts-node": "~10.2.1",
        "tsconfig-paths": "^3.10.1",
        "tslint": "~5.15.0",
        "typescript": "~4.1.6",
        "reflect-metadata": "^0.1.12",
        "wait-on": "^6.0.0",
        "wallaby-webpack": "^3.9.16"
    }
